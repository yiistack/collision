{
    "name": "yiistack/collision",
    "type": "library",
    "description": "collision",
    "keywords": [
        "collision"
    ],
    "homepage": "https://github.com/yiistack/",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/yiistack/collision/issues?state=open",
        "source": "https://github.com/yiistack/collision"
    },
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/di": "3.0.x-dev",
        "yiisoft/yii-console": "3.0.x-dev",
        "nunomaduro/collision": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.5",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Yiistack\\Collision\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiistack\\Collision\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "console": "config/console.php",
            "events-console": "config/events-console.php",
            "providers-console": "config/providers-console.php"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    },
    "config": {
        "sort-packages": true
    }
}
